<table class="table">
    <tr>
        <th>Mentor</th>
        <td>
          <a href="<%= `/users/${mentorship.mentor._id}` %>">
            <%= `${mentorship.mentor.name.first} ${mentorship.mentor.name.last}` %>
          </a>
      </td>
      </tr>
      <tr>
          <th>Mentee</th>
          <td>
            <a href="<%= `/users/${mentorship.mentee._id}` %>">
